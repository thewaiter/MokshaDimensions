// Icon historically from, O/ dir
#define ELMICO(Name, File, Size)                     \
   group { name: "elm/icon/"##Name##"/default"; min: ICMIN ICMIN; max: Size Size; \
   alias: "e/icons/"##Name##; \
   alias: "e/icon/"##Name##"/default"; \
   images.image: "O/"File COMP; parts { \
   part { name: "base"; \
   description { aspect: 1.0 1.0; aspect_preference: BOTH; \
   image.normal: "O/"##File##; } } \
   part { name: "icon";  repeat_events: 1;            \
   description { state: "default" 0.0;                  \
   aspect: 1.0 1.0; aspect_preference: BOTH;               \
   image.normal: "O/"##File## ;                   \
   } } } }

// Icon wiht an Alias
#define ELMICA(Name, Alias, File) \
   group { name: "elm/icon/"##Name##"/default"; min: ICMIN ICMIN; max: 0 0; \
   alias: "elm/icon/"##Alias##"/default"; \
   alias: "e/icons/"##Name##; \
   alias: "e/icon/"##Name##"/default"; \
   alias: "e/icons/"##Alias##; \
   alias: "e/icon/"##Alias##"/default"; \
   images.image: File COMP; parts { part { name: "base"; \
   description { aspect: 1.0 1.0; aspect_preference: BOTH; \
   image.normal: ##File##; } } \
   part { name: "icon"; mouse_events: 0; \
   description { state: "default" 0.0; \
   aspect: 1.0 1.0; aspect_preference: BOTH; \
   image.normal: ##File##; } } } }

#define ELMIC(Name, File) \
   group { name: "elm/icon/"##Name##"/default"; min: ICMIN ICMIN; max: 0 0; \
   alias: "e/icons/"##Name##; \
   alias: "e/icon/"##Name##"/default"; \
   images.image: File COMP; parts { \
   part { name: "base"; \
   description { aspect: 1.0 1.0; aspect_preference: BOTH; \
   image.normal: ##File##; } } \
   part { name: "icon"; mouse_events: 0; \
   description { state: "default" 0.0; \
   aspect: 1.0 1.0; aspect_preference: BOTH; \
   image.normal: ##File##; } } } }

#define ICONMIME(NAME, FILE, SIZE) ELMICO("mimetypes/"NAME, FILE, SIZE)

 #define ELMICSZ(Name, Size, File) \
   group { name: "elm/icon/"##Name##"/default"; min: ICMIN ICMIN; max: 0 0; \
   alias: "e/icons/"##Name##; \
   alias: "e/icon/"##Name##"/default"; \
   images.image: File COMP; parts { \
   part { name: "base"; \
   description { aspect: 1.0 1.0; aspect_preference: BOTH; \
   image.normal: ##File##; } } \
   part { name: "icon"; mouse_events: 0; \
   description { state: "default" 0.0; \
   aspect: 1.0 1.0; aspect_preference: BOTH; \
   image.normal: ##File##; \
   } } } }

